<view><view class="tui-card"><image style="width:100rpx;height:100rpx;border-radius:50rpx;" src="{{detailData.logo}}" mode="aspectFill"></image><view class="column"><view class="tui-order-title">{{detailData.expName}}</view><view data-event-opts="{{[['tap',[['call',['$0'],['detailData.expPhone']]]]]}}" class="row" bindtap="__e"><view class="tui-order-desc">官方电话:</view><view class="tui-order-desc" style="color:#10BE62;">{{detailData.expPhone}}</view></view></view></view><view class="tui-order-tracking"><view class="tui-order-time" style="margin-bottom:12rpx;">{{"快递单号:"+detailData.number}}</view><tui-time-axis vue-id="a81c27d0-1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{detailData.list}}" wx:for-item="data" wx:for-index="index" wx:key="index"><tui-timeaxis-item vue-id="{{('a81c27d0-2-'+index)+','+('a81c27d0-1')}}" backgroundColor="transparent" bind:__l="__l" vue-slots="{{['node','content']}}"><view slot="node"><view class="tui-node" style="{{(index==0?{backgroundColor:backgroundColor}:'')}}"><tui-icon vue-id="{{('a81c27d0-3-'+index)+','+('a81c27d0-2-'+index)}}" name="check" color="#fff" size="{{14}}" bold="{{true}}" bind:__l="__l"></tui-icon></view></view><view slot="content"><view><view class="tui-order-desc">{{data.status}}</view><view class="tui-order-time tui-gray">{{data.time}}</view></view></view></tui-timeaxis-item></block></tui-time-axis></view></view>